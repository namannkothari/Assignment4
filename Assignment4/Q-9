#include <stdio.h>

void main()
{
    int x,y;
    printf("Enter the x coordinates: ");
    scanf("%d",&x);
    
    printf("Enter the y coordinates: ");
    scanf("%d",&y);
    
    if(x >=0 && y >=0)
            printf("Quadrant 1");
    else if(x < 0 && y >= 0)
            printf("Quadrant 2");
    else if(x < 0 && y < 0)
            printf("Quadrant 3");
    else
            printf("Quadrant 4");
    
    
}

